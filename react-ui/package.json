{
  "name": "vexillology-contests-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "axios": "^1.1.3",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "react-lazyload": "^3.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.8.8",
    "sass": "^1.55.0",
    "swr": "^1.3.0",
    "use-persisted-state": "^0.3.3"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "cross-env BROWSER=none PORT=3000 ESLINT_NO_DEV_ERRORS=true npm-run-all -p watch-css start-js",
    "build-js": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build": "npm run build-css && npm run build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "sass --no-source-map src:src",
    "watch-css": "npm run build-css && sass --no-source-map --watch src:src",
    "lint": "eslint \"./src/**/*.{js,jsx}\"",
    "lint-fix": "npm run lint --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint-fix": "npm run stylelint --fix",
    "heroku": "nodemon --exec \"heroku local\" --signal SIGTERM"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:5000",
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "react-error-overlay": "6.0.11",
    "stylelint": "^14.14.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint",
      "prettier-eslint --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  }
}
