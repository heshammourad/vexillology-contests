{
  "name": "vexillology-contests-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.4",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.16.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-lazyload": "^3.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.35.2",
    "swr": "^0.4.0",
    "use-persisted-state": "^0.3.3"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "cross-env BROWSER=none PORT=3000 ESLINT_NO_DEV_ERRORS=true npm-run-all -p watch-css start-js",
    "build-js": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build": "npm run build-css && npm run build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "sass --no-source-map src:src",
    "watch-css": "npm run build-css && sass --no-source-map --watch src:src",
    "lint": "eslint \"./src/**/*.{js,jsx}\"",
    "lint-fix": "npm run lint --fix",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint-fix": "npm run stylelint --fix",
    "heroku": "nodemon --exec \"heroku local\" --signal SIGTERM"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint",
      "prettier-eslint --write",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  }
}
